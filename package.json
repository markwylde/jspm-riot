{
    "name": "jspm-riot",
    "version": "1.0.4",
    "description": "A riot plugin for System.JS and/or JSPM to load tag files while keeping ES6 support",
    "main": "tag",
    "files": [
        "tag.js"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/markwylde/jspm-riot"
    },
    "keywords": [
        "jspm",
        "systemjs",
        "riotjs"
    ],
    "license": "MIT",
    "dependencies": {
        "riot-compiler": "^2.3.23"
    },
    "jspm": {
        "jspmNodeConversion": false,
        "dependencies": {
            "riot-compiler": "npm:riot-compiler@2.3.23"
        },
        "overrides": {
            "github:riot/riot@2.3.18": {
                "main": "riot.js"
            }
        }
    }
}
