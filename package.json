{
    "name": "jspm-riot",
    "version": "2.0.0",
    "description": "A riot plugin for System.JS and/or JSPM to load tag files while keeping ES6 support",
    "main": "tag",
    "files": [
        "tag.js"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/markwylde/jspm-riot"
    },
    "keywords": [
        "jspm",
        "systemjs",
        "riotjs"
    ],
    "license": "MIT",
    "dependencies": {
        "riot-compiler": "^3.0.0-alpha.1"
    },
    "jspm": {
        "jspmNodeConversion": false,
        "dependencies": {
            "riot-compiler": "npm:riot-compiler@3.0.0-alpha.1"
        },
        "overrides": {
            "github:riot/riot@3.0.0-alpha.4": {
                "main": "riot.js"
            }
        }
    }
}
